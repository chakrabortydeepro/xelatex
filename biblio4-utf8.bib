@Book{deli-astr,
  date             = {2012},
  location         = {Louvain},
  publisher        = {Peeters Publishers},
  title            = {Astronomy and Mathematics in Ancient {India} - Astronomie Et Mathématiques De L'inde Ancienne: Actes De La Journée D'études Organisée Le 24 Avril 2009 À \ldots Bruxelles (lettres Orientales Et Classiques)},
  editor           = {Jean-Michel Delire},
