@Article{moer-exam,
  author           = {Daniel Moerman},
  date             = {2011},
  journal          = {The Journal of Mind–Body Regulation},
  number           = {2},
  pages            = {63--72},
  title            = {Examining a Powerful Healing Effect through a Cultural Lens, and Finding Meaning},
  volume           = {1},
  issn             = {1925-1688},
  url              = {http://mbr.synergiesprairies.ca/cjh/index.php/mbr/article/view/476},
  abstract         = {In this paper I argue that the \"placebo effect\" doesn't exist; placebos do, but they are inert so they have no effects (that's what \"inert\" means). Yet we know that often enough, things do happen after placebo administration. Among various causes for such change, I attribute some effects to the meanings the placebos convey to the participants in the medical event — the doctors, nurses, patients, family, community, etc., of the patient. I call these \"meaning responses,\" and survey here some of the ways they occur (with or without the presence of placebos). Then, I describe some recent studies which dramatically complicate the interpretation of RCTs, and our perhaps overly simplistic understandings of the nature of medical efficacy.},
  file             = {:Moerman, Daniel/2011 JMBR Examining a Powerful Healing Effect.pdf:PDF},
  keywords         = {The Meaning Response; Earth years; Lung diseases; Dying on the 4th day of the month; Variability in meaning response; Color; Form; Number; National cultural differences; Historical variation Conclusions},
