@Book{jenk-reth,
  author           = {Keith Jenkins},
  date             = {1991},
  location         = {London and New York},
  publisher        = {Routledge},
  title            = {Re-thinking History},
  isbn             = {0-415-06778-2},
  annotation       = {04.11.30. p.13: " \ldots through hindsight, we in a way know more about the past that the people who lived in it. In translating the past into modern terms and in usig knowledge perhaps previously unavailable, the historian discovers both what has been forgotten about the past and pieces together things never pieced together before. People and social formations are thus caught up in processes that can only be seen in retrospect, and documents and other traces are ripped out of their original contexts of purpose and function to illustrate, say, a pattern which might not be remotely meaningful to any of their authors."\\ p.15: "For while most historians would agree that a rigorous method is important, there is a problem as to which rigorous method they are talking about. \ldots Thus, would you like to follow Hegel or Marx or Dilthey or Weber or Popper or Hempel or Aron or Collingwood or Dray or Oakeshott or Danto or Gallie or Walsh or Atkinson or Leff or Hexter? Would you care to go along with modern empiricists, feminists, the Annales School, neo-Marxists, new-stylists, econometricians, structuralists or post-structuralists, or even Marwick himself, to name but twenty-five possibilities? And this is a short list!\ldots To talk of method as the road to truth is misleading. There is a range of methods without any agreed criteria for choosing." p.16, the so-called ` heartland' of history: `` \ldots `key concepts' of history: time, evidence, cause and effect, continuity and change, and similarity and difference."\\ p.18 argues that we should ask not `what is history' but `who is history for'. History is always partisan. \\ p.18 ``the past can and will sustain countless narratives"\\ p.21, in the section `On Practice', an account of what constitutes the history professionals of the academy and their practices. 1. "themselves personally" 2. ``Second they take their epistemological presuppositions. These are not always held very consciously but historians will have `in mind' ways of gaining `knowledge'. Here will come into play a range of categories -- economic, social, political, cultural, ideological, etc. -- a range of concepts across/within these categories \ldots and broad assumptions about the constancy, or otherwise, of human beings \ldots ." 3. ``historians have routines and procedures\ldots for close working on material: ways of checking it for its origins, position, authenticity, reliability\ldots .", 4. ways of using and interpreting other historians' work, of organising all these elements in new ways and looking for that longed-for `original thesis'. 5. writing up. pressures of the everyday. 6. the reader, consumption of history. p.25: ``history is always history for somone". \\ p.33: ``[History] is never really a matter of the facts \emph{per se} but the weight, position, combination and significance they carry \emph{vis-{\'a}-vis} each other in the construction of explanations that is at issue. \ldots historians transform the events of the past into patterns of meaning that any literal representation of them as facts could never produce."},
  keywords         = {historiography},
  modificationdate = {2018-09-17T00:00:00},
  owner            = {dom},
  source           = {own copy},
}

@Article{jens-nonc,
  author               = {Jensen, Karin B. and Kaptchuk, Ted J. and Kirsch, Irving and Raicek, Jacqueline and Lindstrom, Kara M. and Berna, Chantal and Gollub, Randy L. and Ingvar, Martin and Kong, Jian},
  date                 = {2012},
  journal              = {Proceedings of the National Academy of Sciences},
  title                = {Nonconscious Activation of Placebo and Nocebo Pain Responses},
  doi                  = {10.1073/pnas.1202056109},
  abstract             = {The dominant theories of human placebo effects rely on a notion that consciously perceptible cues, such as verbal information or distinct stimuli in classical conditioning, provide signals that activate placebo effects. However, growing evidence suggest that behavior can be triggered by stimuli presented outside of conscious awareness. Here, we performed two experiments in which the responses to thermal pain stimuli were assessed. The first experiment assessed whether a conditioning paradigm, using clearly visible cues for high and low pain, could induce placebo and nocebo responses. The second experiment, in a separate group of subjects, assessed whether conditioned placebo and nocebo responses could be triggered in response to nonconscious (masked) exposures to the same cues. A total of 40 healthy volunteers (24 female, mean age 23 y) were investigated in a laboratory setting. Participants rated each pain stimulus on a numeric response scale, ranging from 0 = no pain to 100 = worst imaginable pain. Significant placebo and nocebo effects were found in both experiment 1 (using clearly visible stimuli) and experiment 2 (using nonconscious stimuli), indicating that the mechanisms responsible for placebo and nocebo effects can operate without conscious awareness of the triggering cues. This is a unique experimental verification of the influence of nonconscious conditioned stimuli on placebo/nocebo effects and the results challenge the exclusive role of awareness and conscious cognitions in placebo responses.},
  citeulike-article-id = {11223159},
  day                  = {10},
  file                 = {:Jensen, K/2012 Nonconscious activation of placebo and nocebo pain responses PNAS.pdf:PDF},
  keywords             = {placebo},
  modificationdate     = {2019-01-12T00:00:00},
  month                = {9},
  owner                = {dom},
  posted-at            = {2012-09-11 09:30:59},
}

