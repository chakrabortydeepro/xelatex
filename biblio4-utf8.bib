@Article{gode-akas,
  author           = {P. K. Gode},
  date             = {1938},
  journal          = {Karnatak Historical Review},
  number           = {1},
  pages            = {7--18},
  title            = {{Ā}kāśabhairava-kalpa, an Unknown Source of the History of {Vijayanagara}},
  volume           = {V},
  note             = {Cited from \citealt[122--36]{gode-silh2}},
  annotation       = {Based on a BORI copy of the Tanjore MS 6707 (Burnell cat, p.234). The \emph{Sāmrājyalakṣmīpīṭhikā} ``A Tradition of Imperial Glory". This is part of a longer work, the non-tantrik \emph{Ākāśabhairava-Kalpa}, which apparently never got written. Gode, 123--6, gives a summary of the SLP's contents (136 headings). \\ p.126: ``\ldots the central figure of the entire work is the king, and each chapter or paṭala prescribes or describes what the king should do and what he should not do. The work appears to be designed somewhat like a manual on Indian polity with special reference to the practices current at the time the work was composed." \\ Connection with Vijayanagara pointed out by Gode on the basis of 3 references to Vijayanagara: 1. Patala 3, v.\,11 \citep[8]{vasu-samr}: \emph{kṣīrasāgaramadhyasthe śvetadvīpe 'tivistṛte\\ vijayākhyapure ramye cintāmaṇigṛhāgrataḥ//11//} 2. Paṭala 136 (actually paṭala 139, v.\,8\citep[483]{vasu-samr}), and 2. Paṭala 2, v.\,5 \citep[4]{vasu-samr}.\\ Mentions a gun, \emph{nālikā}, in paṭala 105:20--21 (Gode's patala 60) \citep[342]{vasu-samr} . The king should see firework shows, paṭala 107 (Gode's 62), vv.\,29--32 \citep[349]{vasu-samr} Gode says, p.132, this work composed between 1500 and 1565.},
  modificationdate = {2019-01-12T00:00:00},
  month            = {1},
  owner            = {dom},
}

