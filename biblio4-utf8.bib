@Article{smit-poss,
  author           = {Frederick M. J. Smith},
  date             = {2010},
  journal          = {Journal of Ritual Studies},
  pages            = {21--35},
  title            = {Possession, Embodiment, and Ritual in Mental Health Care in India},
  volume           = {24.2},
  abstract         = {In the following account of ritual efficacy, I discuss two ritual strategies for dealing with alleged cases of possession in India that reflect different notions of mental health care within religious contexts. I will supplement these discussions with reflections on a Sanskrit term that I believe is relevant to these two cases, a term that dates to the early centuries CE. The first of the two case studies involves healing at a temple in South India that came to my attention only after my book on deity and spirit possession in India (Smith 2006) was published. The second adds substantive material to discussions in that book (Smith 2006: 114ff., passim). The three are, in order: a description of psychiatric care proffered at a temple called Muthuswamy in Tamil nadu; an autobiographical account of spirit healing that I gathered in 2008 from a woman who had visited the Balaji temple in Mehndipur, Rajasthan; and a re-examination of the Sanskrit term sattvavajaya, which I mentioned in my 2006 book, but did not discuss extensively. In all three, the notion of embodiment is central. In the cases of the Muthuswamy and Balaji tamples, individuals seek cures because they themselves believe or are believed by members of their families to be possessed by untoward spirits that take over their bodies, causing undesired dissociation. The healings at these two temples, which work in entirely different ways, enable the afflicted individuals to regain possession of their bodies, thus unburdening the individual of the offending spirit and the dysfunctionality that it causes. Sattvavayaja, as understood from the Sanskrit texts and interpreted through the lens of a long history of possession and spirit healing in India (Smith 2006), is a general term for a kind of therapy that explicitly addresses notions of embodiment, even if the ayurvedic texts themselves are reticent about the details. Adapted from the source document.},
  file             = {:Smith, Fred/2010 JRS Possession embodiment and ritual in indian mental health care.pdf:PDF},
  modificationdate = {2019-01-12T00:00:00},
  owner            = {dom},
}

