@Article{haag-2005,
  author       = {Pascale Haag},
  date         = {2005},
  journaltitle = {Histoire Épistémologie Langage},
  number       = {1},
  pages        = {127--152},
  title        = {Du nombre grammatical dans les composés sanskrits: le concept d'abhedaikatvasamkhyā},
  volume       = {27},
  doi          = {10.3406/hel.2005.2054},
  abstract     = {Since most of the metalanguage used for Sanskrit grammar was already established by the second century BCE (Patañjali’s Mah bh ya), innovations represent a marginal phenomenon in the P inian tradition. Analysis of compounds, however, and particularly the question of number in complex forms, induces the grammarianphilosopher Bhart hari (5th c.) to formulate a new concept: the •number-‘ one’-withoutdifference” (abhedaikatvasa khy). Indian grammarians regard a compound as an alternative to an analytical phrase, either form being possible. They observe, however, that the number of the subordinate element expressed by the nominal caseending in the analytical phrase does not appear in the corresponding compound (thus, in the compound r ja-puru a, •king’s servant”, the subordinate element r jan -, •king”, could have a singular, a dual or a plural ending). In order to explain how a number qualifying the subordinate element can still be recognized in the absence of a case-ending •even if one does not determine any particular number —, Bhart -hari introduces the notion of abhedaikatvasa khy in the V kyapad ya. This article proposes to reconstruct the history of this concept through the principal treatises of P inian grammar, from Patañjali to N ge a.},
  publisher    = {{PERSEE} Program},
}

@InCollection{haag-2012c,
  author    = {Pascale Haag},
  booktitle = {Studies in the Kasikavrtti. The Section on Pratyaharas: Critical Edition, Translation and Other Contributions},
  date      = {2012},
  editor    = {Pascale Haag and Vincenzo Vergiani},
  location  = {London and New York},
  pages     = {215--242},
  publisher = {Anthem Press},
  title     = {Paratextual Elements in Indian Manuscripts: The Copyists’ Invocations and the Incipit of the Kāśikāvṛtti},
  doi       = {10.7135/upo9781843318927.012},
  isbn      = {9781843318927},
  abstract  = {The undertaking of a new task in India is often held to require a number of rituals, rules, or customs. The field of scholarly activities is no exception in this respect. Alphabetlearning (vidyārambha or akṣarasvīkāra), to start with, is expected to take place on certain auspicious days, following the scriptures' prescriptions. This is also the case with the inception of Vedic recitation, the age of the upanayana ceremony depending on one's caste. Similarly, Sanskrit classical works are often preceded by an auspicious verse (maṃgalaśloka) paying homage, among other things, to a deity and/or to the author's teacher. Such a maṭgalaśloka is believed to ensure that the composition of the work will be completed without hindrance. Since the ritualization of all important events is deeply rooted in the cultural background, it is not surprising that Indian scribes should, in their turn, insert an auspicious note in their manuscripts before the Sanskrit text they are about to copy. The preliminary invocations of the scribes in the manuscripts of the Kāśikāvṛtti available to us will be dealt with in the first part of this paper. The second part is concerned with the historical transmission of the Kāśikāvṛtti's incipit (its maṭgalaśloka), taking into account new elements of information resulting from the study of the manuscripts used for our edition.

Unlike the maṭgalaśloka, the copyist's invocation is not a part of the transmitted text.},
  file      = {:/home/dom/Dropbox/Library/Colleagues Papers/Haag, Pascale/2007 Paratextual elements in Indian Manuscripts.pdf:PDF},
}

