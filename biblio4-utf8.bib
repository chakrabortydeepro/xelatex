@Article{bron-2017,
  author           = {Johannes Bronkhorst},
  date             = {2017},
  journal          = {Contributions to Indian Sociology},
  number           = {3},
  pages            = {361--369},
  title            = {Brahmanism: Its Place in Ancient Indian Society},
  volume           = {51},
  doi              = {10.1177/0069966717717587},
  abstract         = {This article shows how Brahmanism was a regional tradition, confined to the northwestern parts of the Indian subcontinent, that passed through a difficult period—which it barely survived—roughly between the time of Alexander and the beginning of the Common Era. It then reinvented itself, in a different shape. No longer primarily a sacrificial tradition, it became a mainly socio-political ideology that borrowed much (including the belief in rebirth and karmic retribution) from the eastern region in which Buddhism and Jainism had arisen. Its revival went hand in hand with the elaboration of behavioural and theoretical innovations, one of whose purposes was to justify the claimed superiority of Brahmins.},
  file             = {:Bronkhorst, Johannes/2017 CIS Brahmanism_Its_place_in_ancient_Indian_s.pdf:PDF},
  modificationdate = {2021-04-04T06:54:31},
}

