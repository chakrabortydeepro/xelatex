@InCollection{turn-know,
  author           = {turnbull},
  booktitle        = {Encyclopaedia of the History of Science, Technology, and Medicine in non-{Western} Cultures},
  date             = {1997},
  pages            = {485--90},
  title            = {Knowledge Systems: Local Knowledge},
  doi              = {10.1007/978-1-4020-4425-0_8705},
  annotation       = {Argues that knowledge systems are local, and depend upon local, socially-determined networks of agreement and cooperation. Western science is politically and economically powerful and exercises global hegemony. Yet it too is fundamentally relative, local, and only contingently authentic. Cites Geertz's \emph{Interpretation of Cultures} and thick description including that the recognition of the local embeddedness of cultural signs is essential to their understanding. Cites Bruno Latour (1986), ``Visualization and Cognition: thinking with eyes and hands", \emph{Knowledge and Society} 6:1--40. Also v. influenced by Shapin and Schaffer, \emph{Leviathan and the Air Pump}. \\ p.485: ``The sociology of scientific knowledge is one of the most classically post of all modernisms and is therefore an area in which the local is also a thematic presence which is only now coming into focus".\\ p.486: ``Perhaps the most important consequence of the recognition of the localness of scientific knowledge is that it permits a parity in the comparison of the production of contemporary technoscientific knowledge with knowledge production in other cultures. Previously the possibility of a truly equitable comparison was negated by the assumption that indigenous knowledge systems were merely local and were to be evaluated for the extent to which they had scientific characteristics ."},
  modificationdate = {2019-01-12T00:00:00},
  owner            = {dom},
  source           = {WL: HI /(folios) AB/SEL},
}

