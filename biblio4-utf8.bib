@Article{wind-vern,
  author           = {Windhausen, John D.},
  date             = {1964},
  journal          = {Sociology of Education},
  number           = {3},
  pages            = {254--70},
  title            = {The Vernaculars, 1835-1839: A Third Medium for Indian Education},
  volume           = {37},
  issn             = {0038-0407},
  url              = {http://www.jstor.org/stable/2111957},
  abstract         = {In 1835 a decision was made to use government educational funds in British India solely for the promotion of education in the English language. Macaulay's famous eulogy of the English tongue proved decisive in defeating the sponsors of the classical Indian languages. It is not generally recognized that the vernacular languages were then advanced as a third possibility. From 1835 to 1839 William Adam, Brian Houghton Hodgson, Frederick Shore and William Campbell worked to rescind the decree favoring English. Their literary campaign included a defense of the vernaculars against the charge that they were unable to convey modern concepts; and it included, too, the claim that greater numbers would be reached by this medium. Their efforts forced the Governor-General to review the whole question in 1839, and, although the earlier decision was upheld, vernacular education was later financed both in British India and in British Africa.},
  annotation       = {p.257: "The decree itself provided that all funds of the government should thereafter be used for promoting study of the English language. It would not destroy the native schools long as the natives continued to support them. The decree abandoned government stipends for students entering the oriental schools, and no funds were to be alloted for the printing of oriental works. To those who attacked this policy as an attempt to annihilate the languages India, one of the foremost apologists for the Bentinck decree replied: "Is the withholding of direct positive encouragement \ldots equivalent to a direct active discouragement, amounting to a general extermination?" This was, he argued, to only a return a policy of non-interference---alas, a sacred tenet of whiggery!"},
  copyright        = {Copyright © 1964 American Sociological Association},
  file             = {:Windhausen, JD/1964 SoE The Vernaculars 1835-1839 - a third medium for indian education.pdf:PDF},
  keywords         = {indigenous education},
  language         = {English},
  modificationdate = {2019-01-12T00:00:00},
  publisher        = {American Sociological Association},
}

