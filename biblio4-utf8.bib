@Article{hell-2020,
  author   = {Hellwig, Oliver and Scarlata and Salvatore and Ackermann and Elia and Widmer, Paul},
  title    = {The Treebank of Vedic Sanskrit},
  abstract = {This paper introduces the first treebank of Vedic Sanskrit, a morphologically rich ancient Indian language that is of central importance for linguistic and historical research. The selection of the 4,000 sentences contained in this treebank reflects the development of metrical and prose texts over a period of 600 years. We discuss how these sentences are annotated in the Universal Dependencies scheme and which syntactic constructions required special attention. In addition, we describe a syntactic labeler based on neural networks that supports the initial annotation of the treebank, and whose evaluation can be helpful for setting up a full syntactic parser of Vedic Sanskrit.},
  file     = {:Hellwig, Oliver/2020 paper-LREC-2020-1.pdf:PDF},
