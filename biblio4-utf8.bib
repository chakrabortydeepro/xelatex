@Article{meye-revi,
  author           = {Eric Meyer},
  date             = {2006},
  journal          = {Journal of the Royal Asiatic Society},
  number           = {1},
  pages            = {99},
  title            = {Review of: Tagus to Taprobane: Portuguese Impact on the Socio-culture of Sri Lanka from 1505 Ad. by Shihan De Silva Jayasuriya. Pp. 459. Dehiwala (sri Lanka), Tisara Prakasakayo, 2001},
  volume           = {16},
  annotation       = {The history of the Portuguese conquest of Ceylon and its fate (a short lived one as compared with that of Goa) has been amply documented in the works of Paul E. Pieris (Ceylon, the Portuguese Era, 1913-- 14), Tikiri Abeyasinghe (Portuguese Rule in Ceylon, 1966), C.R. Boxer (The Portuguese Seaborne Empire, 1969), Chandra R. de Silva (The Portuguese in Ceylon, 1972) and George D. Winius (The Fatal History of Portuguese Ceylon, 1978). However, the paradox of a long enduring Portuguese influence on Sri Lankan culture in spite of the short duration of the political domination has never been fully explored. This book intends to address the issue and fill a gap in our knowledge of the cultural history of Asia. Its value lies in the use of Portuguese creole texts either available in the Hugh Neville Collection kept at the British Library, or recently collected by the author and others among the so-called 'Portuguese Burgher' and 'Kaffir' communities in Batticaloa and Puttalam. In the first five sections, the reader is provided with a useful but rather patchy overview of the impact of the Portuguese on administration, education, religion, onomastics, dress, cuisine, furniture, and fine arts; the text is interspersed with lists of names and illustrated with translations from Portuguese creole popular poetry and songs. The rest of the book (actually three quarters of it) is devoted to the translation in standard Portuguese and in English of the corpus of the Neville manuscript Portuguese creole texts. Coming after a previous attempt by K.D. Jackson in Sing Without Shame. Oral Traditions in Indo-Portuguese Creole Verse (Amsterdam, 1990) which was considered unsatisfying, this text provides the reader with the basic sources for the knowledge of cultural breeding between Sri Lankan (either Sinhala or Tamil), Portuguese, and African 'Kaffir' folk poetry and music, dancing and theatre, although these latter aspects are not fully addressed in the introductory commentary of the text itself, but rather in the previous more general chapters. What remains to be done is a textual and historical study of that rich corpus, keeping in mind that while determining a chronology is an impossible task so far as oral literature is concerned (see the mentions of Pondicherry and of ``Garibaldi" in the second text which point to late accretions!), intertextual reading provides a useful tool. In that respect, a precise study of the last and longest text (the story of Ourson and Valentine) could possibly yield a large harvest of information regarding how a mediaeval European legend is reinterpreted in a Portuguese colonial context. Eric Meyer Institut National des Langues et Civilisations Orientales, Paris},
  modificationdate = {2019-01-12T00:00:00},
  month            = {4},
  owner            = {dom},
}

