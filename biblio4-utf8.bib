@Article{bast-inve,
  author           = {Cristina Bastos},
  date             = {2002},
  journal          = {Etnogr{\'a}fica},
  number           = {1},
  pages            = {59--76},
  title            = {The Inverted Mirror: Dreams of Imperial Glory and Tales of Subalternity from the Medical School of {Goa}},
  volume           = {VI},
  annotation       = {In this paper I will analyze the identity narratives of Goan doctors under Portuguese colonial rule. Such narratives account for two contrasting styles of self-presentation: first, they emphasize the connection between Goan doctors and the colonial endeavor, portraying military physicians as a kind of civilizing heroe in Africa; second, the narratives point to the marginal and subaltern role of Goan medical school graduates within the Portuguese colonial system. I will explore the relationship between these two opposing styles and suggest how identity-building efforts crystallized in a set of narratives about the history of medicine in India that highlights colonial links and obscures native contributions to medical knowledge. Data will be drawn from interviews with elderly Goan doctors, trained before 1961, from published historical accounts of medical teaching in India, and from documents written either to celebrate the existence of the Medical School of Goa or to analyze and intervene in the situation of Goan doctors serving in the colonies},
  modificationdate = {2019-01-12T00:00:00},
  owner            = {dom},
}

@Article{bast-2004,
  author           = {Cristiana Bastos},
  date             = {2004},
  journal          = {História, Ciências, Saúde-Manguinhos},
  number           = {suppl 1},
  pages            = {11--39},
  title            = {O ensino da medicina na Índia colonial portuguesa: fundação e primeiras décadas da Escola Médico-cirúrgica de Nova Goa [Medical teaching in Portuguese colonial India: the creation and earliest decades of the New Goa Medical-surgical School]},
  volume           = {11},
  doi              = {10.1590/s0104-59702004000400002},
  abstract         = {The centennial celebration of the New Goa Medical-surgical School, held in 1942, glorified the institution's contribution to the consolidation of the Portuguese Empire in Africa. I observe the School from the perspective of the literature on medicine and empire, whose analyzes tend to view it as a tool for exercising biopower. I then question this hypothesis from the perspective of primary sources on the School's first decades, which paint a picture of frailty and administrative disregard that is not very compatible with an imperial project engineered to train physicians and disperse them throughout the colonies. I conclude that the School's creation stemmed from a process where local interests dominated, in a society where the categories "colonizer" and "colonized" were diluted within the complexities of social differentiation. It was with the twentieth-century rewriting of Portuguese colonial history that the narrative of imperial glorification appropriated the School.},
  modificationdate = {2019-01-12T00:00:00},
  publisher        = {{FapUNIFESP} ({SciELO})},
}

