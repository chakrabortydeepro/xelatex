@Article{gode-some,
  author           = {P. K. Gode},
  date             = {1943},
  journal          = {Poona Orientalist},
  number           = {1 \& 2},
  pages            = {1--8},
  title            = {Some {Sanskrit} Verses Regarding the Manufacture of Rose-water Found in a Manuscript of the {Bhojanakutuhala} Dated {Ś}aka 1773 ({Ā}.d. 1851)},
  volume           = {VIII},
  note             = {Cited from \citealt[94--100]{gode-sich1}},
  annotation       = {Gode argues that the Gulābodakaprakaraṇa, part of the Pānīyavarga, was interpolated into the text of the \emph{Bhojanakutūhala} some time between 1803 (a MS does not have it) and 1851 (a MS has it). The passage is cited as granthāntare in Vaidya Raghunathji Indraji (aka Katābhaṭ) in his Nighaṇṭusaṃgraha pulished Junagadh in 1893. \\ Gode says, \cite[94, n.\,2]{gode-some}, that, ``Raghunātha Paṇḍita composed the above lexicon [Rājavyavahārakośa] by order of Shivaji the Great about A.D. 1676". \\ Gode refers to a \emph{Bhiṣagvilāsa} dated September 1942 by Vaidya Bindu Madhava Pandit (of the Ayurvedashram, Ahmednagar), which includes a description of a manuscript of a work called \emph{Hakīm Pharāsisa} by Suta, deposited in the library of the Rajawade Samshodhan Mandir, Dhulia \cite[97\,f., notes 8, 9]{gode-some}. The language of this work is a mixture of Sanskrit, Prakrit, Hindi, Urdu, Marwadi, Gujarati and other languages and dialects. Gode notes two MSS, once dated 1787 and the other dated 1824 (now in BORI). The colophon of the latter reads ``ijrīrajula graṃthe ijula tīva hakīma Pharāsīsa viracītaṃ phiraṃgī hakīma aījulakṛt kītāba hakīmī kī saṃpūrṇa// śake 1746 tāraṇanāma saṃvatsare mārgaśīrṣa vadya 10 daśamī saumyavāsare hakīmī graṃtha samāptaḥ// 6// //".},
  modificationdate = {2019-01-12T00:00:00},
  owner            = {dom},
}

